{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/GameNote.CLI/GameNote.CLI.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
    ],
    "inputs": [
        {
            "id": "cliCommand",
            "description": "The command to run",
            "type": "promptString"
        }
    ]
}